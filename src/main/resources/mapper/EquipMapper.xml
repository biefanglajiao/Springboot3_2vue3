<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot3_2vue3.mapper.equipmapper.EquipMapper">



    <insert id="insertinfo" parameterType="com.example.springboot3_2vue3.domain.equipment.Equipment">
        INSERT INTO equipment (id ,`name`,power,location,state) VALUES (#{id},#{name},#{power},#{location},#{state})
    </insert>

</mapper>




