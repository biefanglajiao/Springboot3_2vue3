<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot3_2vue3.mapper.scheduled.ScheduledcronMapper">


    <select id="findByCronKey" resultType="com.example.springboot3_2vue3.domain.scheduled.Scheduledcron">

        SELECT cronexpression
        FROM scheduledcron

        WHERE  cronkey = #{cronkey}

    </select>


</mapper>




